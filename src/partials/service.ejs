<section class="service">
    <div class="container">
        <h1>What I do</h1>
        <ul id="services">
            <% service.items.forEach(({img, text, url}) => { %>
            <li>
                <a href="<%= url %>" target="_blank">
                    <figure>
                        <img src="img/<%= img %>?width=600&as=webp">
                    </figure>
                    <h4><%= text %> </h4>
                </a>
            </li>
            <% }) %>
        </ul>
    </div>
</section>